void CreateFile(int fileNum, String data) {
  String fileName = "Puzzle" + String(fileNum) + ".txt";
  Serial.print("Creating file: ");
  Serial.println(fileName);
  File f = SPIFFS.open(fileName, "w");
  if (!f) {
    Serial.println("file create open thingy failed");
  } else {
    f.print(data);
    f.close();
  }

}

